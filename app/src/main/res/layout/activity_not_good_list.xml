<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    tools:context="com.ais.patient.activity.main.NotGoodListActivity">
    <include layout="@layout/view_main_title"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/dimens_10">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/dimens_20"
            android:layout_marginBottom="@dimen/dimens_20"
            android:layout_marginLeft="@dimen/dimens_15"
            android:layout_marginRight="@dimen/dimens_15">
            <EditText
                android:id="@+id/et_search"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:hint="请输入疾病名称"
                android:textColorHint="#ff999999"
                android:textSize="14sp"
                android:paddingLeft="@dimen/dimens_10"
                android:paddingTop="@dimen/dimens_13"
                android:paddingBottom="@dimen/dimens_13"
                android:background="@drawable/shape_search_disease"/>
            <TextView
                android:id="@+id/tv_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="确定"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:paddingTop="@dimen/dimens_13"
                android:paddingBottom="@dimen/dimens_13"
                android:paddingLeft="@dimen/dimens_23"
                android:paddingRight="@dimen/dimens_23"
                android:background="@drawable/shape_search_disease_btn"/>
        </LinearLayout>
        <GridView
            android:id="@+id/mNetstedGridView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>
</LinearLayout>
